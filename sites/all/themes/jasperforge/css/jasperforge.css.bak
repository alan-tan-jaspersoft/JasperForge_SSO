/* Default link styles - separate a:visited for a different visited link style */
a,
a:link,
a:visited {
  color: #538DC2;
}

/* Hover/active link styles. Don't forget :focus for accessibility */
a:hover,
a:focus,
a:active {
  color: #538DC2;
  text-decoration: underline;
}

.button a,
.button a:link,
.button a:visited,
a.button,
a.button:link,
a.button:visited {
	
}

.button a:hover,
.button a:active,
a.button:hover,
a.button:active {
	
}

/* removed underline from headings */
h1 > a, h1 > a:link, h1 > a:visited, a h1, a:link h1, a:visited h1,
h2 > a, h2 > a:link, h2 > a:visited, a h2, a:link h2, a:visited h2,
h3 > a, h3 > a:link, h3 > a:visited, a h3, a:link h3, a:visited h3,
h4 > a, h4 > a:link, h4 > a:visited, a h4, a:link h4, a:visited h4,
h5 > a, h5 > a:link, h5 > a:visited, a h5, a:link h5, a:visited h5,
h6 > a, h6 > a:link, h6 > a:visited, a h6, a:link h6, a:visited h6 { 
  text-decoration: none; 
}


